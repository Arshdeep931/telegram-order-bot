# Variables d'environnement pour le bot Telegram
# Copiez ce fichier en .env et remplissez avec vos vraies valeurs

TELEGRAM_BOT_TOKEN=votre_token_ici
ADMIN_ID=votre_id_telegram_ici

# ID du canal Telegram pour recevoir les notifications (optionnel)
# Pour obtenir l'ID: ajoutez le bot au canal comme admin, puis envoyez /get_channel_id dans le canal
CHANNEL_ID=votre_channel_id_ici
